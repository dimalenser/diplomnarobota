<Window x:Class="KolomyyaTrees.SecurityCheck"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KolomyyaTrees"
        mc:Ignorable="d"
        Title="SecurityCheck" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="5*"/>
            <ColumnDefinition Width="83*"/>
        </Grid.ColumnDefinitions>
        <Grid.Background>
            <ImageBrush ImageSource="img/background2blurMore.png"></ImageBrush>
        </Grid.Background>
        <Label Content="Введіть пароль адміністратора" HorizontalAlignment="Left" Margin="150,192,0,0" VerticalAlignment="Top" Grid.Column="1"/>
        <Button Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="222,241,0,0" Grid.Column="1" Click="Button_Click"/>
        <PasswordBox x:Name="passBox" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Column="1" Margin="222,218,0,0" Width="62"/>

    </Grid>
</Window>
